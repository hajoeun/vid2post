# YouTube API 키 (https://console.cloud.google.com/apis/credentials에서 생성)
YOUTUBE_API_KEY=your_youtube_api_key_here

# Ollama API URL (로컬 개발 환경에서는 http://localhost:11434/api)
OLLAMA_API_URL=your_ollama_api_url_here

# Ollama API 사용 가능 여부 (true 또는 false, 기본값은 true)
# Vercel과 같은 서버리스 환경에서는 'false'로 설정
OLLAMA_API_AVAILABLE=true 